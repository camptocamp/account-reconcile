<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_qoqa_buyphrase_tree" model="ir.ui.view">
    <field name="name">qoqa.buyphrase.tree</field>
    <field name="model">qoqa.buyphrase</field>
    <field name="inherit_id" ref="qoqa_offer.view_qoqa_buyphrase_tree"/>
    <field name="arch" type="xml">
      <field name="active" position="after">
        <field name="qoqa_id"/>
      </field>
    </field>
  </record>

  <record id="view_qoqa_buyphrase_form" model="ir.ui.view">
    <field name="name">qoqa.buyphrase.form</field>
    <field name="model">qoqa.buyphrase</field>
    <field name="inherit_id" ref="qoqa_offer.view_qoqa_buyphrase_form"/>
    <field name="arch" type="xml">
      <field name="active" position="after">
        <field name="qoqa_id"/>
      </field>
    </field>
  </record>
</odoo>
