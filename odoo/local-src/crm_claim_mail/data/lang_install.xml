<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record model="base.language.install" id="install_fr_FR">
        <field name="lang">fr_FR</field>
        <field name="state">done</field>
        <field name="overwrite" eval="1"/>
    </record>

    <function
        model="base.language.install"
        name="lang_install"
        eval="[[ref('install_fr_FR')]]"/>

    <record model="base.language.install" id="install_de_DE">
        <field name="lang">de_DE</field>
        <field name="state">done</field>
        <field name="overwrite" eval="1"/>
    </record>

    <function
        model="base.language.install"
        name="lang_install"
        eval="[[ref('install_de_DE')]]"/>

</odoo>
