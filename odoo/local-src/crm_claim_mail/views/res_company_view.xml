<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_qoqa" model="ir.ui.view">
        <field name="name">res.company.form.qoqa</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="qoqa_base_data.view_company_form_qoqa"/>
        <field name="arch" type="xml">
            <page name="qoqa" position="inside">
                <group string="RMA" name="rma">
                    <field name="claim_sale_order_regexp"/>
                </group>
                <group string="Mail" name="mail">
                    <field name="mail_signature_template"/>
                </group>
            </page>
        </field>
    </record>
</odoo>
