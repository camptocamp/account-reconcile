<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
    <!-- INHERIT of qoqa.shop form view -->
    <record id="view_qoqa_shop_form" model="ir.ui.view">
      <field name="name">qoqa.shop</field>
      <field name="model">qoqa.shop</field>
      <field name="inherit_id" ref="qoqa_offer.view_qoqa_shop_form"/>
      <field name="arch" type="xml">
        <group name="shop" position="inside">
            <group>
              <field name="swiss_pp_logo" widget="image"/>
            </group>
        </group>
      </field>
    </record>
  </data>
</openerp>
