<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data noupdate="0">
    <record model="ir.ui.view" id="picking_dispatch_form">
      <field name="name">picking.dispatch.form</field>
      <field name="model">picking.dispatch</field>
      <field name="inherit_id" ref="stock_picking_dispatch.picking_dispatch_form"/>
      <field name="arch" type="xml">
        <button name="action_done" position="attributes">
          <attribute name="states">progress,delayed_done</attribute>
        </button>
        <button name="action_done" position="after">
          <button name="action_delayed_done" states="progress"
            string="Delayed Done"
            class="oe_highlight" type="object"/>
        </button>
      </field>
    </record>
  </data>
</openerp>
