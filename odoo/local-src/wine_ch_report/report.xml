<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <report
      id="wine_ch_inventory"
      name="wine_ch_report.report_wine_inventory"
      file="wine_ch_report.report_wine_inventory"
      model="report.wine_ch_report.cscv_form"
      string="Wine CH Inventory Report"
      report_type="qweb-pdf"
      attachment_use="0"/>

    <report
      id="wine_ch_cscv_form"
      name="wine_ch_report.report_wine_cscv_form"
      file="wine_ch_report.report_wine_cscv_form"
      model="report.wine_ch_report.inventory"
      string="Wine CH CSCV Form Report"
      report_type="qweb-pdf"
      attachment_use="0"/>

    <record id="action_wine_ch_inventory_webkit" model="ir.actions.act_window">
      <field name="name">Wine CH Inventory</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">wine.ch.inventory.wizard</field>
      <field name="view_type">form</field>
      <field name="view_mode">form</field>
      <field name="view_id" ref="wine_ch_inventory_wizard_view"/>
      <field name="target">new</field>
    </record>

    <menuitem name="Wine CH Inventory"
      id="menu_wine_ch_inventory"
      parent="stock.menu_warehouse_report"
      sequence="125"
      action="action_wine_ch_inventory_webkit"
      groups="stock.group_stock_user"/>

    <menuitem
      id="menu_wine_move_analysis"
      name="Wine Moves Analysis"
      sequence="126"
      action="action_wine_move_analysis"
      parent="stock.menu_warehouse_report"/>

</odoo>
