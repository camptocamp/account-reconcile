<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data noupdate="0">
    <record model="ir.ui.view" id="crm_claim_rma_form_view">
      <field name="name">CRM - Claim product return Form</field>
      <field name="model">crm.claim</field>
      <field name="inherit_id" ref="crm_claim_rma.crm_claim_rma_form_view"/>
      <field name="arch" type="xml">
        <button name="%(crm_claim_rma.action_claim_picking_in)d" position="attributes">
          <attribute name="states"></attribute>
        </button>
        <button name="%(crm_claim_rma.action_claim_picking_out)d" position="attributes">
          <attribute name="states"></attribute>
        </button>
        <button name="%(account.action_account_invoice_refund)d" position="attributes">
          <attribute name="states"></attribute>
        </button>
        <xpath expr="//page[@string='Product Return']/group[2]" position="attributes">
          <attribute name="attrs"></attribute>
        </xpath>
      </field>
    </record>
  </data>
</openerp>
