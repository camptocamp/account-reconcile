<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>

    <record id="view_dimension_option_tree" model="ir.ui.view">
      <field name="name">view_dimension_option_tree</field>
      <field name="model">product.variant.dimension.option</field>
      <field name="arch" type="xml">
        <tree string="Dimension option" version="7.0">
          <field name="name" />
          <field name="code" />
        </tree>
      </field>
    </record>

    <record id="view_dimension_option_form" model="ir.ui.view">
      <field name="name">view_dimension_option_form</field>
      <field name="model">product.variant.dimension.option</field>
      <field name="arch" type="xml">
        <form string="Dimension option" version="7.0">
          <group>
            <field name="name" />
            <field name="code" />
            <field name="type_id" />
          </group>
        </form>
      </field>
    </record>

  </data>
</openerp>
