<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">
        <record id="view_delivery_carrier_form" model="ir.ui.view">
            <field name="name">delivery.carrier.form</field>
            <field name="model">delivery.carrier</field>
            <field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
            <field name="arch" type="xml">
                <field name="active" position="after">
                    <group name="connector" string="Connector">
                        <group string="QoQa Service" invisible="[('qoqa_type', '!=', 'service')]">
                            <field name="qoqa_bind_service_ids" nolabel="1"/>
                        </group>
                        <group string="QoQa Rate" invisible="[('qoqa_type', '!=', 'rate')]">
                            <field name="qoqa_bind_rate_ids" nolabel="1"/>
                        </group>
                    </group>
                </field>
            </field>
        </record>
    </data>
</openerp>
